{
	"name": "city_inspector_rest_api",
	"engines": {
		"node": "10.12.0"
	},
	"version": "1.0.2",
	"description": "Rest API to Manage database for City Inspector Projects (Web App and Mobile App)",
	"author": "Damian Wi≈õniewski",
	"license": "ISC",
	"homepage": "https://github.com/damianwisniewski/CityInspectorRestApi/docs#readme",
	"main": "app/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/damianwisniewski/CityInspectorRestApi.git"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=PRODUCTION node app/index.js",
		"start:dev": "nodemon app/index.js --colorful --logs=$npm_config_logs",
		"start:chrome-inspect": "nodemon --inspect=0.0.0.0:9229 app/index.js --logs=$npm_config_logs",
		"db-sync": "node scripts/sync_database.js",
		"db-sync:force": "node scripts/sync_database.js --force",
		"db-fill": "node scripts/fill_database.js",
		"test": "mocha \"./app/**/*.test.js\"",
		"test:watch": "npm test -- --watch",
		"test:coverage": "nyc report",
		"test:system": "mocha --exit \"./system_tests/**/*.test.js\"",
		"lint": "eslint app/**/*.js",
		"postversion": "git add package.json && git commit -m v$npm_package_version"
	},
	"husky": {
		"hooks": {}
	},
	"nyc": {
		"include": [
			"app/**/*.js"
		],
		"exclude": [
			"tests/**/*.test.js"
		],
		"all": true
	},
	"devDependencies": {
		"@types/express": "^4.17.1",
		"chai": "^4.2.0",
		"chai-http": "^4.2.1",
		"eslint": "^5.15.0",
		"eslint-config-prettier": "^6.1.0",
		"eslint-plugin-prettier": "^3.1.0",
		"faker": "^4.1.0",
		"find-process": "^1.4.2",
		"husky": "^1.3.1",
		"mocha": "^6.0.2",
		"nodemon": "^1.18.10",
		"nyc": "^13.3.0",
		"prettier": "1.16.4",
		"sequelize-cli": "^5.4.0",
		"sequelize-test-helpers": "^1.1.2",
		"sinon": "^7.3.2",
		"sinon-chai": "^3.3.0"
	},
	"dependencies": {
		"chalk": "^2.4.2",
		"cross-env": "^5.2.0",
		"body-parser": "^1.18.3",
		"cloudinary": "^1.13.2",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"express-validator": "6.2.0",
		"helmet": "^3.18.0",
		"jsonwebtoken": "^8.5.1",
		"multer": "^1.4.2",
		"mysql2": "^1.6.5",
		"nodemailer": "^5.1.1",
		"sequelize": "^5.8.12",
		"uuid": "^3.3.2"
	}
}