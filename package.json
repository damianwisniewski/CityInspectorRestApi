{
	"name": "city_inspector_rest_api",
	"version": "1.0.0",
	"description": "Rest API to Manage database for City Inspector Projects (Web App and Mobile App)",
	"author": "Damian Wi≈õniewski",
	"license": "ISC",
	"homepage": "https://github.com/damianwisniewski/CityInspectorRestApi/docs#readme",
	"main": "app/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/damianwisniewski/CityInspectorRestApi.git"
	},
	"scripts": {
		"db-migrate": "cross-env NODE_ENV=development sequelize db:migrate",
		"start": "cross-env NODE_ENV=PRODUCTION node app/index.js",
		"start:dev": "cross-env NODE_ENV=DEVELOPMENT nodemon app/index.js",
		"test": "npm run test:unit && npm run test:endpoint",
		"test:coverage": "nyc report",
		"test:unit": "cross-env NODE_ENV=TEST nyc mocha tests/units/**/*.test.js",
		"test:endpoint": "cross-env NODE_ENV=TEST mocha tests/endpoints/**/*.test.js",
		"lint": "eslint **/*",
		"release": ""
	},
	"husky": {
		"hooks": {
			"pre-commit": "echo \"Hook lint later\" \"Hook test here later\""
		}
	},
	"nyc": {
		"include": [
			"app/**/*.js"
		],
		"exclude": [
			"tests/**/*.test.js"
		],
		"all": true
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-http": "^4.2.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.15.0",
		"husky": "^1.3.1",
		"mocha": "^6.0.2",
		"nodemon": "^1.18.10",
		"nyc": "^13.3.0",
		"prettier": "1.16.4"
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"cloudinary": "^1.13.2",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"express-validator": "^5.3.1",
		"faker": "^4.1.0",
		"jsonwebtoken": "^8.5.0",
		"mysql2": "^1.6.5",
		"nodemailer": "^5.1.1",
		"sequelize": "^4.43.0",
		"sequelize-cli": "^5.4.0",
		"uuid": "^3.3.2"
	}
}
